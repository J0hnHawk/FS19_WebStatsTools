        <i3dMapping id="hydraulicLeftPipe2Ref" node="0>0|1|0|5" />
        <i3dMapping id="hydraulicRightPipe1Ref" node="0>0|1|0|6" />
        <i3dMapping id="hydraulicRightPipe2Ref" node="0>0|1|0|7" />
