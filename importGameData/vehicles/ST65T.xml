        <i3dMapping id="hydraulicLeftPunch" node="0>0|2|0|0" />
        <i3dMapping id="hydraulicRight" node="0>0|2|1" />
        <i3dMapping id="hydraulicRightPunch" node="0>0|2|1|0" />
    </i3dMappings>
</vehicle>
