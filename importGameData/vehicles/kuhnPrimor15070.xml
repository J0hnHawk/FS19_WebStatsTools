            <animationNode node="drum2" rotSpeed="250"  rotAxis="1" turnOnFadeTime="3" turnOffFadeTime="4"/>
            <animationNode node="drum3" rotSpeed="500"  rotAxis="3" turnOnFadeTime="4" turnOffFadeTime="6"/>
