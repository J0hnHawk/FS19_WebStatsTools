            <part node="supportArm"  startTime="0" duration="0.6" startRot="0 0 0" endRot="-35 0 0" />
            <part node="supportFeet" startTime="0" duration="0.6" startRot="0 0 0" endRot="30 0 0" />
        </animation>

        <animation name="folding">
            <!-- lowering -->

            <!-- axis -->
            <part node="axisRot"        startTime="0" endTime="1" startRot="5 0 0" endRot="15 0 0"/>
            <part node="axisRot"        startTime="1" endTime="2" startRot="15 0 0" endRot="0 0 0"/>

            <!-- eradicators -->
            <part node="eradicatorBack"  startTime="0" endTime="1" startRot="-20 0 0" endRot="75 0 0"/>
            <part node="eradicatorFront" startTime="0" endTime="1" startRot="0 0 0" endRot="70 0 0"/>

            <!-- front shares -->
            <part node="leftFrontShares"  startTime="0" endTime="1" startRot="0 -90 0" endRot="75 -90 0"/>
            <part node="rightFrontShares" startTime="0" endTime="1" startRot="0 90 0"  endRot="75 90 0"/>

            <!-- lift arms -->
            <part node="leftLiftArm"  startTime="0" endTime="1" startRot="-8 -90 0" endRot="0 -90 0"/>
            <part node="leftFrame"    startTime="0" endTime="1" startRot="8 0 0" endRot="0 0 0"/>
            <part node="leftFrame"    startTime="0" endTime="1" startTrans="0.055 -0.429 -1.65" endTrans="0.055 -0.449 -1.62"/>

            <part node="leftOrientationArms"  startTime="0" endTime="1" startRot="-10 0 0" endRot="0 0 0"/>
            <part node="leftShareUnits"       startTime="0" endTime="1" startRot="10 0 0" endRot="0 0 0"/>


            <part node="rightLiftArm"  startTime="0" endTime="1" startRot="-8 90 0" endRot="0 90 0"/>
            <part node="rightFrame"    startTime="0" endTime="1" startRot="8 0 0" endRot="0 0 0"/>
            <part node="rightFrame"    startTime="0" endTime="1" startTrans="-0.055 -0.429 -1.65" endTrans="-0.055 -0.449 -1.62"/>

            <part node="rightOrientationArms" startTime="0" endTime="1" startRot="-10 0 0" endRot="0 0 0"/>
            <part node="rightShareUnits"      startTime="0" endTime="1" startRot="10 0 0" endRot="0 0 0"/>

            <!-- folding -->

            <!-- lift parts up -->
            <part node="mainArm"            startTime="1" endTime="4" startRot="0 0 0" endRot="90 0 0"/>

            <part node="leftBarLockRot"     startTime="1" endTime="4" startRot="0 -90 0" endRot="-90 -90 0"/>
            <part node="rightBarLockRot"    startTime="1" endTime="4" startRot="0 90 0" endRot="-90 90 0"/>

            <!-- fold to back -->
            <part node="leftComponentJoint"  startTime="4" endTime="7" startRot="0 0 0" endRot="0 0 -90"/>
            <part node="rightComponentJoint" startTime="4" endTime="7" startRot="0 0 0" endRot="0 0 90"/>

            <!-- bar lock -->
            <part node="leftBarLockArm_locked" startTime="1" endTime="1.01" visibility="true"/>
            <part node="leftBarLockArm_locked" startTime="1.01" endTime="7.5" visibility="false"/>
            <part node="leftBarLockArm_unlocked" startTime="1" endTime="1.01" visibility="false"/>
            <part node="leftBarLockArm_unlocked" startTime="1.01" endTime="7.5" visibility="true"/>

            <part node="rightBarLockArm_locked" startTime="1" endTime="1.01" visibility="true"/>
            <part node="rightBarLockArm_locked" startTime="1.01" endTime="7.5" visibility="false"/>
            <part node="rightBarLockArm_unlocked" startTime="1" endTime="1.01" visibility="false"/>
            <part node="rightBarLockArm_unlocked" startTime="1.01" endTime="7.5" visibility="true"/>

            <part node="leftBarLock_locked" startTime="1" endTime="1.01" visibility="true"/>
            <part node="leftBarLock_locked" startTime="1.01" endTime="7.5" visibility="false"/>
            <part node="rightBarLock_locked" startTime="1" endTime="1.01" visibility="true"/>
            <part node="rightBarLock_locked" startTime="1.01" endTime="7.5" visibility="false"/>
            <part node="leftBarLock_unlocked" startTime="1" endTime="1.01" visibility="false"/>
            <part node="leftBarLock_unlocked" startTime="1.01" endTime="7.5" visibility="true"/>
            <part node="rightBarLock_unlocked" startTime="1" endTime="1.01" visibility="false"/>
            <part node="rightBarLock_unlocked" startTime="1.01" endTime="7.5" visibility="true"/>

            <part node="leftLockArm" startTime="1" endTime="1.3" startRot="0 0 0" endRot="0 -25 0"/>
            <part node="rightLockArm" startTime="1" endTime="1.3" startRot="0 0 0" endRot="0 25 0"/>

            <!-- connect components together -->
            <part componentJointIndex="3" startTime="7" endTime="7.5" startRotLimit="0 0 0" endRotLimit="0 15 180" />
            <part componentJointIndex="4" startTime="7" endTime="7.5" startRotLimit="0 0 0" endRotLimit="0 15 180" />
            <part node="connectorJointConnector1" startTime="7" endTime="7.5" startRot="0 0 0" endRot="0 0 90"/>

            <part componentJointIndex="3" startTime="5.5" endTime="7" startTransLimit="20 20 20" endTransLimit="5 5 5" />
            <part componentJointIndex="3" startTime="7" endTime="7.2" startTransLimit="5 5 5" endTransLimit="1 1 1" />
            <part componentJointIndex="3" startTime="7.2" endTime="7.5" startTransLimit="1 1 1" endTransLimit="0 0 0" />

            <part componentJointIndex="1" startTime="1" endTime="2" startRotLimit="0 0 20" endRotLimit="0 0 0" />
            <part componentJointIndex="2" startTime="1" endTime="2" startRotLimit="0 0 20" endRotLimit="0 0 0" />

            <part componentJointIndex="1" startTime="7.25" endTime="7.5" startRotLimit="0 0 0" endRotLimit="0 5 45" />
            <part componentJointIndex="2" startTime="7.25" endTime="7.5" startRotLimit="0 0 0" endRotLimit="0 5 45" />

            <part componentJointIndex="1" startTime="0" endTime="1" startTransLimit="0 0.1 0" endTransLimit="0 0 0" />
            <part componentJointIndex="2" startTime="0" endTime="1" startTransLimit="0 0.1 0" endTransLimit="0 0 0" />

            <part node="lockArm_workMode" startTime="1" endTime="7.49" visibility="true"/>
            <part node="lockArm_workMode" startTime="7.49" endTime="7.5" visibility="false"/>

            <part node="lockArm_transportMode" startTime="1" endTime="7.49" visibility="false"/>
            <part node="lockArm_transportMode" startTime="7.49" endTime="7.5" visibility="true"/>
        </animation>
    </animations>

