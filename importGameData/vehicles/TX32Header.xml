            <part node="innerCanolaKnifes" startTime="0.5" endTime="1"  startTrans="0 -0.03 0"  endTrans="0 0 0" />
            <part node="outerCanolaKnifes" startTime="0" endTime="0.5"  startTrans="0 0 0"  endTrans="0 0.03 0" />
