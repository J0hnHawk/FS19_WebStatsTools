        </animation>
    </animations>

    <cylindered>
        <movingTools>
            <movingTool node="armDummy" delayedNode="arm">
                <controls axis="AXIS_FRONTLOADER_ARM" invertAxis="true" mouseSpeedFactor="1.2" iconName="FRONTLOADER_ARM_ROTATE" />
                <attacherJoint jointIndices="1" />
